@model IEnumerable<CarServiceWeb.Models.ClientModel>

@{
    ViewBag.Title = "Список клиентов";
}

<h2>Список клиентов</h2>

<p>
    @Html.ActionLink("Добавить новую клиентскую базу", "Index")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PhoneNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OrderDone)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PhoneNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OrderDone)
        </td>
    </tr>
}

</table>
